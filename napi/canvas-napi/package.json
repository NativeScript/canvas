{
	"name": "canvas-napi",
	"version": "0.0.0",
	"main": "index.js",
	"types": "index.d.ts",
	"napi": {
		"name": "canvas-napi",
		"triples": {
			"defaults": false,
			"additional": [
				"aarch64-apple-darwin",
				"x86_64-apple-darwin",
				"universal-apple-darwin"
			]
		}
	},
	"license": "MIT",
	"dependencies": {
		"@nativescript/macos-node-api": "0.1.3"
	},
	"devDependencies": {
		"@napi-rs/cli": "^2.18.4",
		"ava": "^6.0.1"
	},
	"ava": {
		"timeout": "3m"
	},
	"engines": {
		"node": ">= 10"
	},
	"scripts": {
		"artifacts": "napi artifacts",
		"build": "napi build --platform --release",
		"build:debug": "napi build --platform",
		"prepublishOnly": "napi prepublish -t npm",
		"test": "ava",
		"universal": "napi universal",
		"version": "napi version",
		"build-tsc": "tsc -p tsconfig.json"
	}
}
