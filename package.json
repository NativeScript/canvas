    "pixi.js": "^5.3.3",
    "playcanvas": "^1.37.0",
    "react-nativescript": "^2.0.0",
    "rimraf": "^3.0.2",
    "rxjs": "~6.6.0",
    "svelte-native": "^0.9.3",
    "three": "^0.123.0",
    "typescript": "4.3.5",
    "vexflow": "^3.0.9",
    "xmldom": "~0.6.0",
